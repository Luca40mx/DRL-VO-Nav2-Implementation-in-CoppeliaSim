<?xml version="1.0"?>
<robot name="simple_box_bot">

  <material name="colore_bianco">
    <color rgba="1 1 1 0.8"/> </material>
  
  <material name="colore_rosso">
    <color rgba="1 0 0 1"/> </material>


  <link name="base_footprint">
    
    <visual>
      <geometry>
        <box size="0.25 0.15 0.1"/> 
      </geometry>
      <material name="colore_bianco"/>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
    </visual>

    <visual>
      <geometry>
        <box size="0.05 0.05 0.05"/> 
      </geometry>
      <material name="colore_rosso"/>
      <origin xyz="0.125 0 0.10" rpy="0 0 0"/>
    </visual>

  </link>
</robot>
